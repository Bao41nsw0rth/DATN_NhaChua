  id: bucThu01
  itemName: "Th\u01B0 tuy\u1EC7t m\u1EC7nh"
  itemDescription: "L\xE1 th\u01B0 h\xECnh nh\u01B0 vi\u1EBFt cho m\xECnh"
  itemPrefab: {fileID: 4035940929292758424, guid: 9f667c8adc32aa24b88f698796a029e8,
